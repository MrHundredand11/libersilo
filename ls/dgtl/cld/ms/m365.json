                            "insc_term": "Office 365 ProPlus",
                            "scion_id": "ls_dgtl_cld_ms_m365_srvc_o365pp",
                            "scions": [
                                {
                                    "insc_type": "stm",
                                    "insc_tag": "Definitions",
                                    "insc_ttl":"<span class='styl_stm_ttl_spprt'>Definitions</span>&nbsp;of&nbsp;(Office 365 ProPlus)",
                                    "scion_id": "ls_dgtl_cld_ms_m365_srvc_o365pp_dfntns",
                                    "scions": [
                                        {
                                            "insc_type": "blade",
                                            "insc_tag": "Definition",
                                            "insc_term": "Office 365 ProPlus",
                                            "insc_cntn": "is Microsoft's suite of productivity applications that includes Microsoft Word, Excel, PowerPoint, and Outlook for both Windows and Mac machines."
                                        }
                                    ]
                                },
                                {
                                    "insc_type": "stm",
                                    "insc_tag": "Installation",
                                    "insc_ttl": "<span class='styl_stm_ttl_spprt'>Installation</span>&nbsp;of&nbsp;(Office 365 ProPlus)",
                                    "scion_id": "ls_dgtl_cld_ms_m365_srvc_o365pp_instll",
                                    "scions": [
                                        {
                                            "insc_type": "blade",
                                            "insc_tag": "Installation",
                                            "insc_term": "Office 365 ProPlus",
                                            "insc_cntn": "is installed locally on the user's device. It is <span class='styl_in_txt_bld'>not</span> a web-based version of Office."
                                        },
                                        {
                                            "insc_type": "blade",
                                            "insc_tag": "Installation",
                                            "insc_cntn": "You can install <span class='styl_in_txt_inlineterm_01'>Office 365 ProPlus</span>&nbsp;right&nbsp;<span class='styl_in_txt_undr'>from the internet<span>&nbsp;or&nbsp;<span class='styl_in_txt_undr'>from a shared location</span>&nbsp;on the local network."
                                        },
                                        {
                                            "insc_type": "blade",
                                            "insc_tag": "Updates",
                                            "insc_term": "Office 365 ProPlus",
                                            "insc_cntn": "is updated on a&nbsp;<span class='styl_in_txt_undr'>semi-annual basis</span>&nbsp;or on a&nbsp;<span class='styl_in_txt_undr'>monthly basis</span>"
                                        }
                                    ]
                                },
                                {
                                    "insc_type": "stm",
                                    "insc_tag": "Comparisons",
                                    "insc_ttl": "<span class='styl_stm_ttl_spprt'>Comparisons</span>&nbsp;of&nbsp;(Office Versions)",
                                    "scion_id": "ls_dgtl_cld_ms_m365_srvc_o365pp_cmprsns",
                                    "scions": [
                                        {
                                            "insc_type": "blade",
                                            "insc_tag": "Comparison",
                                            "insc_cntn": "<span class='styl_in_txt_inlineterm_01'>Office 365 ProPlus</span>&nbsp;is updated with new features on a regular basis, while&nbsp;<span class='styl_in_txt_inlineterm_01'>Office Professional Plus 2019</span>&nbsp;features remain the same."
                                        },
                                        {
                                            "insc_type": "blade",
                                            "insc_tag": "Comparison",
                                            "insc_cntn": "Users can install&nbsp;<span class='styl_in_txt_inlineterm_01'>Office 365 ProPlus</span>&nbsp;on multiple devices with just a single license, while&nbsp;<span class='styl_in_txt_inlineterm_01'>Office Professional Plus 2019</span>&nbsp;is limited to one device per license."
                                        },
                                        {
                                            "insc_type": "blade",
                                            "insc_tag": "Comparison",
                                            "insc_cntn": "<span class='styl_in_txt_inlineterm_01'>Office 365 ProPlus</span>&nbsp;is activated by connecting to the internet.&nbsp;<span class='styl_in_txt_inlineterm_01'>Office Professional Plus 2019</span>&nbsp;is activated through volume activation methods."
                                        }
                                    ]
                                },
