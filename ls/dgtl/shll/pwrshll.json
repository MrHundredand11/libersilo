                    "scions": [
                        {
                            "insc_typ1": "insc",
                            "insc_typ2": "txt",
                            "insc_cntn": "[\t<span class='styl_in_txt_code'>Install-Module -Name ExchangeOnlineManagement -RequiredVersion 3.1.0</span>\t]"
                        },
                        {
                            "insc_typ1": "insc",
                            "insc_typ2": "txt",
                            "insc_cntn": "[\t<span class='styl_in_txt_code'>Import-Module ExchangeOnlineManagement</span>\t]"
                        },
                        {
                            "insc_typ1": "insc",
                            "insc_typ2": "txt",
                            "insc_cntn": "[\t<span class='styl_in_txt_code'>Set-Execution Policy RemoteSigned -Force</span>\t]"
                        },
                        {
                            "insc_typ1": "insc",
                            "insc_typ2": "txt",
                            "insc_cntn": "[\t<span class='styl_in_txt_code'>$UserCredential = Get-Credential</span>\t]"
                        },
                        {
                            "insc_typ1": "insc",
                            "insc_typ2": "txt",
                            "insc_cntn": "[\t<span class='styl_in_txt_code'>$Session = New-PSSession -ConfigurationName Microsoft.Exchange\n-ConnectionUri https://outlook.office365.com/powershell-liveid/\n-Credential $UserCredential -Authentication Basic -AllowRedirection</span>\t]"
                        },
                        {
                            "insc_typ1": "insc",
                            "insc_typ2": "txt",
                            "insc_cntn": "[\t<span class='styl_in_txt_code'>Import-PSSession $Session</span>\t]"
                        }
                    ]
                },
