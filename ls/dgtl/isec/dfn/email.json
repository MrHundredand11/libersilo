            "type": "branch",
            "scions": [
                {
                    "insc_name": "What is the Sender Policy Framework?",
                    "insc_type": "stm_list",
                    "scion_id": "ls_dgtl_isec_dfn_email_spf_whatis",
                    "scions": [
                        {
                            "insc_type": "blade",
                            "insc_tag": "Abbreviation",
                            "insc_term": "SPF",
                            "insc_cntn": "<b>S</b>ender <b>P</b>olicy <b>F</b>ramework"
                        },
                        {
                            "insc_type": "blade",
                            "insc_tag": "Definition",
                            "insc_term": "Sender Policy Framework",
                            "insc_cntn": "is an e-mail authentication protocol that checks the sender's IP address against a list of IPs located on the domain listed in the Return Path header of the e-mail. This list is known as the SPF record."
                        }
                    ]
                },
                {
                    "insc_name": "SPF Failures",
                    "insc_type": "stm_list",
                    "scion_id": "ls_dgtl_isec_dfn_email_spf_failures",
                    "scions": [
                        {
                            "insc_type": "blade",
                            "insc_tag": "Definition",
                            "insc_term": "SPF Failure",
                            "insc_cntn": "is when the sender's IP address is not found in the SPF record."
                        },
                        {
                            "insc_type": "blade",
                            "insc_tag": "Result",
                            "insc_term": "SPF Failure",
                            "insc_cntn": "means that the e-mail is either <u>sent to spam</u> or <u>discarded altogether</u>."
                        },
                        {
                            "insc_type": "stm_olst_exp",
                            "insc_term": "<span class='span_list_title'>Types</span> of <span class='span_opn_cls'>(</span><span class='span_obj_name'>SPF Failure</span><span class='span_opn_cls'>)</span> ::",
                            "insc_tag": "Failure Types",
                            "scion_id": "ls_dgtl_isec_dfn_email_spf_failures_types",
                            "scions": [
                                {
                                    "insc_term": "SPF Hard Fail",
                                    "insc_type": "entry"
                                },
                                {
                                    "insc_term": "SPF Soft Fail",
                                    "insc_type": "entry"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
