                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt",
                                    "insc_offs": "styl_offs_lft_73",
                                    "insc_cntn": "\"<span class='styl_insc_txt_trm_grg styl_insc_txt_size_17'>Mem</span>\"&nbsp;(<span class='styl_insc_txt_stkitc'>Hebrew</span>) = \"<span class='styl_insc_txt_stkitc'>water</span>\""
                                },
