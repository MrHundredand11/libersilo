{
    "ls_id": "LS-LULS-THLMA-CX-L049",
    "ls_ttl": "Liber 49 = The Book of Babalon",
    "ls_url": "/ls/luls/thlma/cx/l049.json",
    "scion_id": "ls_luls_thlma_cx_l049",
    "scions": [
        {
            "drct_id": "TXTS",
            "drct_ttl": "Texts + Translations of 'The Book of Babalon'",
            "scion_id": "ls_luls_thlma_cx_l049_txts",
            "scions": [
                {
                    "insc_typ1": "stm",
                    "insc_typ2": "lst_exp",
                    "insc_ttl": "List of Texts + Translations of (Liber 49)",
                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst",
                    "scions": [
                        {
                            "insc_term": "Transmission on 28-Feb-1946",
                            "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00",
                            "scions": [
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "1",
                                    "insc_cntn": "[\tYea, it is I, BABALON.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v001"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "2",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tAnd this is my book, that is the fourth chapter of the Book of the Law, He completing the Name, for I am out of NUIT by HORUS, the incestuous sister of RA-HOOR-KHUIT.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v002"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "3",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tIt is BABALON. TIME IS. Ye fools.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v003"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "4",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tThou hast called me, oh accursed and beloved fool.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v004"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "5-8",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "(\t<i>missing and presumed lost</i>\t)",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v005to8"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "9",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tNow know that I, BABALON, would take flesh and come among men.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v009"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "10",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tI will come as a penelous flame, as a devious song, a trumpet in judgement halls, a banner before armies.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v010"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "11",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tAnd gather my children unto me, for THE TIME is at hand.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v011"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "12",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tAnd this is the way of my incarnation. Heed!\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v012"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "13",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tThou shalt offer all thou art and all thou hast at my altar, witholding nothing. And thou shalt be smitten full sore and thereafter thou shalt be outcast and accursed, a lonely wanderer in abominable places.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v014"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "14",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tYe Dare. I have asked of none other, nor have they asked. Else is vain. But thou hast willed it.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v014"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "15",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tKnow then that thus I came to thee before, thou a great Lord, and I a maid enrapt. Ah blind folly.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v015"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "16",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tAnd thereafter madness, all in vain. Thus it has been, multi-form. How thou hast burned beyond.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v016"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "17",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tI shall come again, in the form thou knowest. Now it shall be thy blood.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v017"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "18",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tThe altar is aright, and the robe.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v018"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "19",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tThe perfume is sandal, and the cloth green and gold. There is my cup, our book, and thy dagger.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v019"
                                },
                                {
                                    "insc_typ1": "insc",
                                    "insc_typ2": "txt_itr",
                                    "insc_itr": "20",
                                    "insc_offs": "styl_offs_dwn_23",
                                    "insc_cntn": "[\tThere is a flame.\t]",
                                    "scion_id": "ls_luls_thlma_cx_l049_txts_lst_00_v020"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
